<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="payment_provider_qpaypro" model="payment.provider">
        <field name="name">QPayPro</field>
        <field name="image_128"
               type="base64"
               file="payment_qpaypro/static/src/img/qpaypro_icon.png"/>
        <field name="module_id" ref="base.module_payment_qpaypro"/>
        <field name="payment_method_ids"
               eval="[(6, 0, [
                   ref('payment.payment_method_card'),
               ])]"/>
        <field name="code">qpaypro</field>
        <field name="redirect_form_view_id" ref="redirect_form"/>
    </record>

</odoo>
